{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
  ],
  "automerge": true,
  "automergeType": "branch",
  "labels": ["Dependency"],
  "minimumReleaseAge": "1 week",
  "packageRules": [
    {
      "automerge": true,
      "automergeType": "branch",
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "minimumReleaseAge": "1 week",
    }
  ],
  "lockFileMaintenance":{
    "enabled": true,
    "automerge": true,
  },
  "cdnurl": {
    "fileMatch": [
      "^\\.esbuild\\.mjs$"
    ]
  },
}
